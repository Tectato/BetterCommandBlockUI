{
  "schemaVersion": 1,
  "id": "bettercommandblockui",
  "version": "${version}",

  "name": "BetterCommandBlockUI",
  "description": "Provides a more usable UI for command blocks",
  "authors": [
    "Tec"
  ],

  "license": "CC0-1.0",
  "icon": "assets/bettercommandblockui/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "bettercommandblockui.main.BetterCommandBlockUI"
    ],
    "modmenu": [
      "bettercommandblockui.main.ModMenuIntegration"
    ]
  },
  "mixins": [
    "bettercommandblockui.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "minecraft": ">=1.21.6",
    "fabric-api": ">=0.80.0"
